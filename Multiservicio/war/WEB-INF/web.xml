<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Multiservicio</servlet-name>
		<servlet-class>cerdo.unsa.MultiservicioServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Multiservicio</servlet-name>
		<url-pattern>/multiservicio</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Sesion</servlet-name>()
		<servlet-class>cerdo.unsa.Sesion</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Sesion</servlet-name>
		<url-pattern>/sesion</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Codigo</servlet-name>
		<servlet-class>cerdo.unsa.Codigo</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Codigo</servlet-name>
		<url-pattern>/codigo</url-pattern>
	</servlet-mapping>
	
	
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>